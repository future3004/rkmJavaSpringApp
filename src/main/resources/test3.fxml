<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Label?>

<?import javafx.scene.text.Text?>
<?import javafx.scene.control.TextField?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.SecondaryController">
    <children>
        <Label layoutX="256.0" layoutY="10.0" text="DELETE EMPLOYEE(S)" />
        <Text layoutX="150.0" layoutY="70.0" text="Employee Email : " />
        <TextField fx:id="del_email" promptText="email" layoutX="270.0" layoutY="54.0" />
        <Button layoutX="500.0" layoutY="54.0" mnemonicParsing="false" onAction="#deleteSubmit" text="SUBMIT"/>

        <Label layoutX="240.0" layoutY="110.0" text="Employee Data/Info" />

        <Text layoutX="180.0" layoutY="150.0"  text="Employee ID : " />
        <Text fx:id="del_id" layoutX="300.0" layoutY="150.0" />
        <Text layoutX="180.0" layoutY="185.0"  text="First Name : " />
        <Text fx:id="del_fName" layoutX="300.0" layoutY="185.0" strokeType="OUTSIDE" strokeWidth="0.0"  wrappingWidth="69.677734375" />
        <Text layoutX="180.0" layoutY="215.0"  text="Last Name : " />
        <Text fx:id="del_LName" layoutX="300.0" layoutY="215.0"   />
        <Text layoutX="180.0" layoutY="245.0"  text="Email : " />
        <Text fx:id="deleteEmailText" layoutX="300.0" layoutY="245.0"   />
        <Text layoutX="180.0" layoutY="275.0"  text="Phone Number : " />
        <Text fx:id="deletePhoneText" layoutX="300.0" layoutY="275.0"  />

        <Button layoutX="200.0" layoutY="300.0" mnemonicParsing="false" onAction="#performDelete" text="DELETE"/>
        <Button fx:id="deleteBtn" layoutX="353.0" layoutY="300.0" mnemonicParsing="false" onAction="#deleteCancel" text="CANCEL"/>
        <Button fx:id="homeBtn" layoutX="253.0" layoutY="350.0" mnemonicParsing="false" onAction="#backToEmpManager" text="Employee Home" />
    </children>
</AnchorPane>

